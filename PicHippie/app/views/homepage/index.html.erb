<div class="container">
	<% @image.each do|photos|%>
		<% if check_image_link(get_image(photos.url)) %>
		<div class="row">
        <div class="col s12 m10">
          <div class="card">
            <div class="card-image">
              <img src=<%= get_image(photos.url) %> >
              <span class="card-title"><%=photos.owner%></span>
            </div>
            <div class="card-action">
              <a href="www.pexels.com">Explore</a>
            </div>
          </div>
        </div>
      </div>
      <% end %>
	<% end %>
	<%= will_paginate @image, renderer: MaterializePagination::Rails %>
</div>

	